{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "sourceMap": false,
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "importHelpers": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": false,
    "lib": ["es5", "es6", "es7", "esnext", "dom"],
    "types": ["node"],
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "*": ["node_modules/white-web-sdk/types/*"]
    }
  },
  "exclude": [
    "node_modules",
  ],
  "include": [
    "src"
  ]
}
